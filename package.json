{
  "name": "net-tools",
  "version": "1.0.0",
  "description": "Networking Tools",
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc && cpx \"src/**/*.{html,png,jpg,ico,icns,svg}\" dist",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "package": "npm run build && electron-packager . \"Networking Tools\" --all --overwrite --icon ./dist/app.icns --ignore=/src",
    "dev": "concurrently --kill-others \"tsc -w\" \"cpx \\\"src/**/*.{html,png,jpg,ico,icns,svg}\\\" dist --watch\" \"electron --remote-debugging-port=9222 ./dist/main.js\""
  },
  "repository": "https://github.com/tewarid/net-tools-electron",
  "keywords": [
    "Electron",
    "Networking",
    "Tool",
    "Git",
    "GitLab"
  ],
  "author": "Devendra Tewari",
  "license": "MIT",
  "devDependencies": {
    "@types/codemirror": "0.0.74",
    "@types/jquery": "^3.3.29",
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "electron": "^4.1.4",
    "electron-packager": "^13.1.1",
    "tslint": "^5.10.0",
    "typescript": "^3.4"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "codemirror": "^5.46.0",
    "gitlab": "5.0.0-rc.15",
    "jquery": "^3.4.0",
    "knockout": "^3.5.0",
    "popper.js": "^1.15.0",
    "xterm": "^3.12.2"
  }
}
